{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "env": {
    "ENVIRONMENT": "prod",
    "VECTOR_STORE_DIR": "/tmp/vectorstore",
    "MLFLOW_TRACKING_URI": "/tmp/mlflow"
  }
}

